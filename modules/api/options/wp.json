{
  "baseURI": "https://demo.wp-api.org/wp-json",
  "api": {
    "meta": {
      "get": {
        "/": "/"
      }
    },
    "post": {
      "get": {
        "/posts": "/main/v2/posts",
        "/posts/:id": "/main/v2/posts"
      },
      "post": {
        "/posts": "/main/v2/posts"
      },
      "put": {
        "/posts/:id": "/main/v2/posts"
      },
      "delete": {
        "/posts/:id": "/main/v2/posts"
      }
    },
    "category": {
      "get": {
        "/categories": "/main/v2/categories",
        "/categories/:id": "/main/v2/categories"
      },
      "post": {
        "/categories": "/main/v2/categories"
      },
      "put": {
        "/categories/:id": "/main/v2/categories"
      },
      "delete": {
        "/categories/:id": "/main/v2/categories"
      }
    },
    "page": {
      "get": {
        "/pages": "/main/v2/pages",
        "/pages/:id": "/main/v2/pages"
      },
      "post": {
        "/pages": "/main/v2/pages"
      },
      "put": {
        "/pages/:id": "/main/v2/pages"
      },
      "delete": {
        "/pages/:id": "/main/v2/pages"
      }
    },
    "sale": {
      "get": {
        "/posts": "/main/v2/posts",
        "/posts/:id": "/main/v2/posts"
      },
      "post": {
        "/posts": "/main/v2/posts"
      },
      "put": {
        "/posts/:id": "/main/v2/posts"
      },
      "delete": {
        "/posts/:id": "/main/v2/posts"
      }
    }
  },
  "map": {
    "post": {
      "base": "",
      "props": {
        "id": "id",
        "title": "title.rendered",
        "content": "content.rendered",
        "excerpt": "excerpt.rendered",
        "type": "type",
        "slug": "slug",
        "date": "date",
        "categories": "",
        "images": {
          "extends": "image",
          "base": "_embedded.wp:featuredmedia"
        },
        "terms": {
          "extends": "term",
          "base": "_embedded.wp:term",
          "props": {
          }
        },
        "author": {
          "one": true,
          "extends": "author",
          "base": "_embedded.author",
          "props": {
          }
        }
      }
    },
    "page": {
      "extends": "post"
    },
    "category": {
      "extends": "term"
    },
    "term": {
      "id": "id",
      "count": "count",
      "description": "description",
      "link": "link",
      "name": "name",
      "slug": "slug",
      "parent": "parent"
    },
    "image": {
      "id": "id",
      "title": "title.rendered",
      "caption": "caption.rendered",
      "alt": "alt_text",
      "url": "source_url",
      "sizes": "media_details.sizes"
    },
    "author": {
      "id": "id",
      "name": "name",
      "url": "url",
      "description": "description",
      "link": "link"
    }
  }
}